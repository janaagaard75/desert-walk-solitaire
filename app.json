{
  "expo": {
    "name": "Desert Walk",
    "description": "A game of solitaire. Source code at https://github.com/janaagaard75/desert-walk",
    "slug": "desert-walk",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "version": "0.0.0",
    "orientation": "default",
    "primaryColor": "#464",
    "icon": "./assets/icons/app-icon.png",
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": [
        "mp4",
        "ttf"
      ],
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}